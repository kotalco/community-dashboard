    "joi": "^17.4.0",
    "next": "^11.0.0",
    "postcss": "^8.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.7.1",
    "react-redux": "^7.2.3",
    "swr": "^0.5.6",
